{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "preserve",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "baseUrl": ".",  // Add this
      "paths": {       // Add this
        "@/*": ["./*"]
      },
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ]
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      ".next/types/**/*.ts",
      "next-env.d.ts",
      "app/redux/MapCRUDProvider.tsx"
, "app/components/MapComponent/MapComponent.tsx"    ],
    "exclude": [
      "node_modules"
    ]
  }